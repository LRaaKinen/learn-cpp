#include <iostream>

int main() {

  std::cout << " K   K  M   M \n";
  std::cout << " K  K   MM MM \n";
  std::cout << " K K    MM MM \n";
  std::cout << " KK     M M M \n";
  std::cout << " K K    M   M \n";
  std::cout << " K  K   M   M \n";
  std::cout << " K   K  M   M \n";

  return 0;

}
